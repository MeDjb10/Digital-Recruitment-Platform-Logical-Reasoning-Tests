{
  "name": "test-service",
  "version": "1.0.0",
  "description": "Test Management Microservice",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "seed": "node src/scripts/seed.js"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/controllers/**/*.js",
      "!src/controllers/**/*.test.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/*.test.js"
    ]
  },
  "dependencies": {
    "amqplib": "^0.10.8",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "helmet": "^5.1.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.3.4"
  },
  "author": "Your Name",
  "license": "MIT"
}
